#aside {
    padding: 0;
    margin: 0;
    height: 100%;
}

#main {
    padding: 0;
    margin: 0;
    width: 100%;
    height: 100%;
}

.dashboard
.navMenu { position: absolute; }


.dashboard {
    z-index: 1;
    padding: 0px 0px 0px 0px;
    margin: 0;
    width: 100%;
    height: 100%;
}


.resizable-handle {
    z-index: 2;
    position: absolute;
    width: 20px;
    height: 20px;
    bottom: 0;
    right: 0;
    background: url("data:image/svg+xml;base64,") no-repeat bottom right !important;
    padding: 0 7px 7px 0;
    background-origin: content-box;
    box-sizing: border-box;
    cursor: se-resize;
}

.react-grid-item-header {
    display: flex;
    align-items: center;
    height: 40px;
    margin: 0;
    border-bottom: 1px solid #e2e2e2;
    text-align: center;
    position: relative;
    cursor: move;
}

.react-grid-item-header-title-container {
    position: relative;
    font-size: 16px;
    text-align: center;
    width: 100%;
    outline: none;
    flex: 1;
    display: flex;
    align-items: center;
    justify-content: center;
    height: inherit;
    min-width: 0;
    min-height: 40px;
}

.react-grid-item-header-title {
    width: 100%;
    padding: 0px 0px;
    margin: 0;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    font-size: 16px;
}

.react-grid-item-header-commandBar-container {
    display: flex;
    height: 40px;
    opacity: 0;
    transition: opacity 0.25s;
    position: absolute;
    top: 0;
    right: 0;
    z-index: 13;
    align-items: center;
}

.react-grid-item-header-commandBar-container:hover { opacity: 1; }


.navMenu {
    z-index: 2;
    padding: 0;
    margin: 0;
    width: 30px;
}



.react-grid-item {
    border-style: solid;
    border-width: 1px;
    border-color: black;
}



.mapboxDiv {
    padding: 0;
    margin: 0;
    width: 100%;
    height: 100%;
}

.active {
    border-style: solid;
    border-width: 1px;
    border-color: red;
}

.inactive {
    border-style: solid;
    border-width: 1px;
    border-color: #000000;
    border-color: rgba(0, 0, 0, 0);
}

