{
    "name": "engineeringtoolsserver",
    "version": "0.5.0",
    "private": true,
    "scripts": {
        "wasm": "-O3 --converge --runtime full|stub|none --maximumMemory --sharedMemory",
        "build-wasm-debug": "asc.cmd bin/index.ts --target debug",
        "build-wasm-release": "asc.cmd bin/index.ts --target release",
        "build-wasm": "npm run build-wasm-debug && npm run build-wasm-release",
        "build-tsc": "tsc --build --pretty",
        "build-css": "postcss.cmd -o ../css/mapbox-gl.css ../css/post-mapbox-gl.css",
        "build": "gulp --continue --gulpfile ./gulpfile.js",
        "build-webpack": "webpack --mode development --config webpack.config.js",
        "deploy": "npm run build-tsc && webpack --mode production --config webpack.config.js",
        "lint": "eslint.cmd --quiet --fix \"src/**/*.ts?(x)\"",
        "pretty": "npx prettier --write --config .prettierrc ./src",
        "tsviz": "tsviz -d -r src/DataTypes/ diagram.png"
    },
    "dependencies": {
        "@assemblyscript/loader": "^0.14.9",
